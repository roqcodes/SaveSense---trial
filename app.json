{
  "expo": {
    "name": "savesense",
    "slug": "savesense",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.maketon.savesense",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "savesense",
    "plugins": [
      "expo-web-browser",
      "expo-secure-store",
      "./plugins/withNativeBackgroundShare"
    ]
  }
}